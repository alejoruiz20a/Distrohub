---
import Header from "./Header.astro"
import Footer from "./Footer.astro"

interface Props {
    title: string;
}
const {title} = Astro.props;
---

<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width" />
        <meta name="generator" content={Astro.generator} />
        <title>{title}</title>
    </head>
    <body>
        <Header></Header>
        <slot/>
        <Footer></Footer>
    </body>
</html>